@model BusinessObjects.Notification.NotificationProcessingStatus
@{
    Layout = null;
}
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 top-offset-10">
                    <p class="texthead">Processing Status</p>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-12">
                    <hr class="customehr" />
                </div>
            </div>
            <div class="row top-offset-20">
                @if (Model.objPendingFullText != null)
                {
                    <div class="col-xs-12 col-sm-6 col-md-3">
                        <div class="col-xs-12 col-sm-12 shadowbox Processingbox">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 text-center head">
                                    <p class="font-size-16 top-offset-10 font-weight-600">Pending Full Text (s)</p>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-12 text-center">
                                @if (Convert.ToInt64(@Model.objPendingFullText.Sum(x => Int32.Parse(x.Count.Length > 0 ? x.Count : "0"))) > 0)
                                {
                                    <h1 class="totaltext"><a href="@Url.Action("Index", "NotificationList", new { PageIndex = 1, PageSize = 10, StatusId = 1 })">@(Model.objPendingFullText.Sum(x => Int32.Parse(x.Count.Length > 0 ? x.Count : "0")))</a> <sub class="subtext">Total</sub></h1>
                                }
                                else
                                {
                                    <h1 class="totaltext">@(Model.objPendingFullText.Sum(x => Int32.Parse(x.Count.Length > 0 ? x.Count : "0"))) <sub class="subtext">Total</sub></h1>
                                }
                            </div>
                            <div class="row bottom-border">
                                <div class="col-xs-12 col-sm-12">
                                    <div class="row top-offset-10">
                                        @if (Model.objPendingFullText.Count > 0)
                                        {
                                            <div class="col-xs-8 col-sm-8"><p>@Model.objPendingFullText[0].Text</p></div>
                                            if (Convert.ToInt32(Model.objPendingFullText[0].Count) > 0)
                                            {
                                                <div class="col-xs-4 col-sm-4 text-right processingcount"><p><a href="@Url.Action("Index", "NotificationList", new { PageIndex = 1, PageSize = 10, StatusId = 1 })">@Model.objPendingFullText[0].Count</a></p></div>
                                            }
                                            else
                                            {
                                                <div class="col-xs-4 col-sm-4 text-right processingcount"><p>@Model.objPendingFullText[0].Count</p></div>
                                            }
                                        }
                                        else
                                        {
                                            <div class="col-xs-8 col-sm-8"><p>Mail not sent</p></div>
                                            <div class="col-xs-4 col-sm-4 text-right processingcount"><p>0</p></div>
                                        }

                                    </div>
                                </div>
                            </div>
                            <div class="row bottom-border">
                                <div class="col-xs-12 col-sm-12">
                                    <div class="row top-offset-10">
                                        @if (Model.objPendingFullText.Count > 1)
                                        {
                                            <div class="col-xs-8 col-sm-8"><p>@Model.objPendingFullText[1].Text</p></div>
                                            if (Convert.ToInt32(Model.objPendingFullText[1].Count) > 0)
                                            {
                                                <div class="col-xs-4 col-sm-4 text-right processingcount"><p><a href="@Url.Action("Index", "NotificationList", new { PageIndex = 1, PageSize = 10, StatusId = 1 })">@Model.objPendingFullText[1].Count</a></p></div>
                                            }
                                            else
                                            {
                                                <div class="col-xs-4 col-sm-4 text-right processingcount"><p>@Model.objPendingFullText[1].Count</p></div>
                                            }
                                        }
                                        else
                                        {
                                            <div class="col-xs-8 col-sm-8"><p>Response not received</p></div>
                                            <div class="col-xs-4 col-sm-4 text-right processingcount"><p>0</p></div>
                                        }
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                }
