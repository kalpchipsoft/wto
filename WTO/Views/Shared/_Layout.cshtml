<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*  @Styles.Render("~/contents/css/css")*@
    <link href="~/contents/css/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/contents/css/css/style.min.css" rel="stylesheet" />
    <link href="~/contents/css/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/contents/css/css/comman.css" rel="stylesheet" />
    <link href="~/contents/css/css/wto.css" rel="stylesheet" />
    <script src="~/contents/js/jquery.min.js"></script>
    <script src="~/contents/js/bootstrap.js"></script>
    <script src="~/contents/js/jstree.min.js"></script>
    <script src="~/contents/js/jquery-ui.js"></script>
    <script src="~/contents/js/WTOHTMLJS.js?v=1"></script>
    <script src="~/contents/js/Validation.js"></script>
    <script src="~/JQuery/master.js"></script>
    @* @Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <div id="wrapper" class="">
        <div id="page-content-wrapper">

            <!-- menu section start -->

            <nav class="navbar navbar-default wto-menu">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <button type="button" class="hamburger animated fadeInLeft" id="toggleOpenClose" data-toggle="offcanvas">
                            <span class="hamb-top"></span>
                            <span class="hamb-middle"></span>
                            <span class="hamb-bottom"></span>
                        </button>
                        <a class="navbar-brand MenuHeading" href="#">SPS/ TBT Notification Management System</a>

                    </div>
                    <nav class="navbar navbar-inverse navbar-fixed-top" id="sidebar-wrapper" role="navigation">

                        <ul class="nav sidebar-nav">
                            <button type="button" class="hamburger small-hamburger animated fadeInLeft is-closed visible-xs" data-toggle="offcanvas">
                                <span class="hamb-top"></span>
                                <span class="hamb-middle"></span>
                                <span class="hamb-bottom"></span>
                            </button>
                            <li class="sidebar-brand bottom-offset-20 top-offset-20">
                                <a href="#">
                                    <img src="~/contents/img/wto-logo.png" alt="WTOlogo" class="WTOlogo img-responsive " />
                                </a>
                            </li>
                            <li class="top-offset-50 list dashboard">
                                <a href="~/WTO/WTODashboard">
                                    <img src="~/contents/img/dashboard.png" alt="dashboard" /><span class="submenulist">Dashboard</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                            <li class="list notification">
                                <a href="@Url.Action("GetNotificationsMasters", "NotificationList", new { CallFrom = "" })">
                                    <img src="~/contents/img/notification1.png" alt="notification" /><span class="submenulist">Notification List</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                            <li class="list Addnotification">
                                <a href="@Url.Action("Edit_Notification", "AddNotification", new { Id = 0 })">
                                    <img src="~/contents/img/Addnotification.png" alt="Addnotification" /><span class="submenulist">Add Notification</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                            <li class="list AddMOMList">
                                <a href="@Url.Action("Index","MOMList")">
                                    <img src="~/contents/img/AddMOMB.png" alt="Addnotification" /><span class="submenulist">MOM List</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                            <li class="list AddMOM">
                                <a href="@Url.Action("Index","AddMOM")">
                                    <img src="~/contents/img/AddMOMB.png" alt="Addnotification" /><span class="submenulist">Add MOM</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                            <li class="list ManageAccess">
                                <a href="@Url.Action("Index", "ManageAccess")">
                                    <img src="~/contents/img/manageB.png" alt="Addnotification" /><span class="submenulist">Manage Access</span> @*<span class="glyphicon glyphicon-chevron-right pull-right"></span>*@
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <div class="collapse navbar-collapse" id="myNavbar">
                        @{ Html.RenderAction("UserInfo", "Partial"); }
                    </div>
                </div>
            </nav>
            <!-- menu section End -->
            @RenderBody()
        </div>
    </div>
    @*  @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/contents/js")*@
    @RenderSection("scripts", required: false)
    
    <input id="hdnNoProgress" type="hidden" value="0" />
    <div id="loader-wrapper">
        <div id="loader"></div>
    </div>
</body>
</html>
