@model BusinessObjects.Notification.EditNotification
@{
    ViewBag.Title = "Add Notification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/contents/css/css/bootstrap-combobox.css" rel="stylesheet" />
<script src="~/contents/js/bootstrap-combobox.js"></script>
<script src="~/JQuery/AddUpdateNotification.js?v=2"></script>
<!-- body section start -->
@{
    bool IsSPS = false;
    bool IsTBT = false;
    if (@Model.Noti_Type != null && Convert.ToString(@Model.Noti_Type).IndexOf("1") > -1)
    {
        IsSPS = true;
    }
    if (@Model.Noti_Type != null && Convert.ToString(@Model.Noti_Type).IndexOf("2") > -1)
    {
        IsTBT = true;
    }
}
<input type="hidden" name="HiddenHSCode" id="HiddenHSCode" value='@Model.HSCodes'>
<input type="hidden" name="HiddenHSCode" id="hdnNotificationId" value='@Model.NotificationId'>
@Html.Hidden("RedirectTo", Url.Action("GetNotificationsMasters", "NotificationList"))
<div class="container-fluid bodybackground">
    <div class="padding-bottom-40">
        <div class="col-xs-12 col-sm-12 col-md-12">
            <div class="col-xs-12 col-sm-12 col-md-12 top-offset-20 bottom-offset-20">
                <div class="row">
                    <h4 class="blue-color"><b>Add Notification</b></h4>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 bottom-offset-20">
                <div class="row">
                    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

                        <!-- First Accordian part start -->
                        <div class="panel panel-default Accordian-panel">
                            <div class="panel-heading" role="tab" id="headingOne">
                                <h4 class="panel-title Accordian-panel-title">
                                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                        <i class="more-less glyphicon glyphicon-chevron-right"></i>
                                        Notification
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                <div class="panel-body Accordian-body">
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-12 whitebackground padding-bottom-10 full-border top-border-none">
                                            <div class="row top-offset-20">
                                                <div class="col-xs-12 col-sm-12 col-md-12">
                                                    <div class="fileinput fileinput-new upload pull-right" data-provides="fileinput">
                                                        <span class="btn btn-light-blue btn-padding btn-file"><span class="glyphicon glyphicon-open"></span> <span class="puttext">Upload File</span><input type="file" class="fileholder uploadfile" id="filename"></span>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Notification Type<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-bold" id="NotificationTypeId">
                                                        <b>
                                                            @if (IsSPS)
                                                            {
                                                            <label class="checkbox-inline"><input type="checkbox" value="1" id="TypeSPS" checked="checked">SPS</label>
                                                            }
                                                            else
                                                            {
                                                            <label class="checkbox-inline"><input type="checkbox" value="1" id="TypeSPS">SPS</label>
                                                            }
                                                            @if (IsTBT)
                                                            {
                                                            <label class="checkbox-inline"><input type="checkbox" value="2" id="TypeTBT" checked="checked">TBT</label>
                                                            }
                                                            else
                                                            {
                                                            <label class="checkbox-inline"><input type="checkbox" value="2" id="TypeTBT">TBT</label>
                                                            }
                                                        </b>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Notification Number<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">
                                                        @if (@Model.Noti_Number != null)
                                                        {
                                                        <input type="text" id="NotificationNumberId" class="form-control textboxcontrol" value='@Model.Noti_Number' />
                                                        }
                                                        else
                                                        {
                                                        <input type="text" id="NotificationNumberId" class="form-control textboxcontrol" />
                                                        }
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Notification Status<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @Html.DropDownListFor(m => Model.Noti_Status, new SelectList(Model.Noti_TypeList, "Id", "Type"), "Select status", new { Id = "NotificationStatusId", Class = "form-control textboxcontrol" })
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Date of Notification<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.NotificationId > 0)
                                                        {
                                                        <input type="text" id="DateofNotificationId" class="form-control date-picker textboxcontrol" value='@Convert.ToDateTime(@Model.Noti_Date).ToLongDateString()' />
                                                        }
                                                        else
                                                        {
                                                        <input type="text" id="DateofNotificationId" class="form-control date-picker textboxcontrol" value="" />
                                                        }
                                                        <i class="glyphicon glyphicon-calendar form-control-feedback blue-color"></i>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Final Date for Comments<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.NotificationId > 0)
                                                        {
                                                        <input type="text" id="FinalDateforCommentsId" class="form-control date-picker textboxcontrol" value='@Convert.ToDateTime(@Model.FinalDateOfComments).ToLongDateString()' />
                                                        }
                                                        else
                                                        {
                                                        <input type="text" id="FinalDateforCommentsId" class="form-control date-picker textboxcontrol" value="" />
                                                        }
                                                        <i class="glyphicon glyphicon-calendar form-control-feedback blue-color"></i>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Send Response By<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.NotificationId > 0)
                                                        {
                                                        <input type="text" id="SendResponseById" class="form-control date-picker textboxcontrol" value='@Convert.ToDateTime(@Model.SendResponseBy).ToLongDateString()' />
                                                        }
                                                        else
                                                        {
                                                        <input type="text" id="SendResponseById" class="form-control date-picker textboxcontrol" value="" />
                                                        }
                                                        <i class="glyphicon glyphicon-calendar form-control-feedback blue-color"></i>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-xs-12 col-sm-offset-4 col-sm-8 col-md-offset-3 col-md-9">
                                                    <p class="text-danger hidden" id="SendResponseError" style="font-size:12px;">Send Response By date is lesser than date of notificaton please check !</p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Notifying Country<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.CountryList != null)
                                                        {
                                                        @Html.DropDownListFor(m => Model.Noti_Country, new SelectList(Model.CountryList, "CountryId", "Name"), "Select country", new { Id = "NotifyingCountryId", Class = "form-control textboxcontrol" })
                                                        }
                                                        else
                                                        {
                                                        <select id="NotifyingCountryId" class="form-control textboxcontrol">No Country</select>
                                                        }
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Title<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.Title != null)
                                                        {
                                                        <textarea class="form-control textboxcontrol" cols="30" rows="2" id="TitleId">@Model.Title</textarea>
                                                        }
                                                        else
                                                        {
                                                        <textarea class="form-control textboxcontrol" cols="30" rows="2" id="TitleId"></textarea>
                                                        }
                                                        <span class="pull-right label label-default" id="count_message"><span id="display_count">0</span>/<span id="word_left">100</span></span>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Agency Responsible<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.ResponsibleAgency != null)
                                                        {
                                                        <textarea class="form-control textboxcontrol" cols="30" rows="3" id="AgencyResponsibleId">@Model.ResponsibleAgency</textarea>
                                                        }
                                                        else
                                                        {
                                                        <textarea class="form-control textboxcontrol" cols="30" rows="3" id="AgencyResponsibleId"></textarea>
                                                        }
                                                        <span class="pull-right label label-default" id="count_message"><span id="display_count1">0</span>/<span id="word_left1">150</span></span>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Notification Under Article<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.Noti_UnderArticle != null)
                                                        {
                                                        <input type="text" id="NotificationUnderArticleId" maxlength="30" class="form-control textboxcontrol" value='@Model.Noti_UnderArticle' />
                                                        }
                                                        else
                                                        {
                                                        <input type="text" id="NotificationUnderArticleId" maxlength="30" class="form-control textboxcontrol" />
                                                        }
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Products Covered<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.ProductsCovered != null)
                                                        {
                                                        <textarea id="ProductsCoveredId" class="form-control textboxcontrol" cols="30" rows="3">@Model.ProductsCovered</textarea>
                                                        }
                                                        else
                                                        {
                                                        <textarea id="ProductsCoveredId" class="form-control textboxcontrol" cols="30" rows="3"></textarea>
                                                        }
                                                        <span class="pull-right label label-default" id="count_message"><span id="display_count2">0</span>/<span id="word_left2">150</span></span>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">HS Codes<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        <a href="#" class="btn sky-blue" data-toggle="modal" data-target="#selecthr" onclick="bindhscodeUId();">Select HS Code</a>
                                                    </p>
                                                    <div class='table-responsive hs-code-table @(@Model.SelectedHSCodes != null ? "" : "hidden")'>
                                                        <table class="table table-bordered top-offset-10" id="HSCodesId">
                                                            <thead>
                                                                <tr>
                                                                    <th class="col-md-2"><b>HS Code</b></th>
                                                                    <th class="col-md-9"><b>Name</b></th>
                                                                    <th class="col-md-1"></th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @if (@Model.SelectedHSCodes != null)
                                                                {
                                                                    foreach (var Item in @Model.SelectedHSCodes)
                                                                    {
                                                                <tr class='@Item.HSCode'>
                                                                    <td>@Item.HSCode</td>
                                                                    <td>@Item.Text</td>
                                                                    <td>
                                                                        <a href='#@Item.HSCode' id='@Item.HSCode' onclick='return RemoveHSCodeRow(this.id);' class='remove-icon'><span class='glyphicon glyphicon-remove'></span></a>
                                                                    </td>
                                                                </tr>
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Description of Content<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        @if (@Model.Description != null)
                                                        {
                                                        <textarea id="DescriptionofContentId" class="form-control textboxcontrol" cols="30" rows="4">@Model.Description</textarea>
                                                        }
                                                        else
                                                        {
                                                        <textarea id="DescriptionofContentId" class="form-control textboxcontrol" cols="30" rows="4"></textarea>
                                                        }
                                                        <span class="pull-right label label-default" id="count_message"><span id="display_count3">0</span>/<span id="word_left3">300</span></span>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Second Accordian part start -->
                        <div class='panel panel-default Accordian-panel @(@Model.NotificationId <= 0 ? "hidden" : "")'>
                            <div class="panel-heading" role="tab" id="headingTwo">
                                <h4 class="panel-title Accordian-panel-title">
                                    <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                        <i class="more-less glyphicon glyphicon-chevron-right"></i>
                                        Documents
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseTwo" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingTwo">
                                <div class="panel-body Accordian-body">
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-12 whitebackground padding-bottom-10 full-border top-border-none">
                                            <div class="row top-offset-10">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Do You Have a Detailed Notification?<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal top-offset-20">
                                                        <div class="btn-group btn-toggle" data-toggle="buttons">
                                                            <label class="btn switch-white yes">
                                                                <input type="radio" name="Documents" value="1"> Yes
                                                            </label>
                                                            <label class="btn switch-white No">
                                                                <input type="radio" name="Documents" value="0"> No
                                                            </label>
                                                        </div>&nbsp;
                                                        <a href="#" class="btn sky-blue mailenquiry">Mail to Enquiry Desk</a>
                                                        <div class="fileinput fileinput-new hidden upload" data-provides="fileinput">
                                                            <span class="btn sky-blue btn-file"><span>Upload File</span><input type="file" class="fileholder1" id="filename" /></span>
                                                            <span class="fileinput-filename"></span><span class="fileinput-new dark-blue underline" id="choose1">No File Choose</span>
                                                        </div>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10 selectlangusge">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Select Language<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-3 col-md-3">
                                                    <p class="wto-label-normal">
                                                        <select class="form-control textboxcontrol" id="sel1">
                                                            <option value="-1">--Select Language--</option>
                                                            <option value="0">Chinese, Roman</option>
                                                            <option value="1" selected>Chinese</option>
                                                            <option value="3">Chinese, English</option>
                                                        </select>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10 selectlangusge">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Select Translator<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <div class="wto-label-normal">
                                                        <div class="dropdown">
                                                            <a href="#" class="btn col-sm-5 transletorDrop col-md-4 dropdown-toggle " data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="value pull-left">-- Select Translator --</span> <span class="caret dropcaret"></span></a>
                                                            <div class="dropdown-menu notify-drop">
                                                                <div class="drop-content">
                                                                    <div class="element">
                                                                        <div class="col-xs-12 col-sm-12 col-md-12 translatename">
                                                                            <p class="wto-label-bold blue-color name">Aman Ahuja</p>
                                                                            <p>Proficient in Language - <b>Chinese, Roman</b></p>
                                                                            <hr>
                                                                            <p>File Pending for Translation -  5</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="element">
                                                                        <div class="col-xs-12 col-sm-12 col-md-12 translatename">
                                                                            <p class="wto-label-bold blue-color name">Jyoti Gupta</p>
                                                                            <p>Proficient in Language - <b>Chinese</b></p>
                                                                            <hr>
                                                                            <p>File Pending for Translation -  0</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="element">
                                                                        <div class="col-xs-12 col-sm-12 col-md-12 translatename">
                                                                            <p class="wto-label-bold blue-color name">Goutam Sen</p>
                                                                            <p>Proficient in Language - <b>Chinese, English</b></p>
                                                                            <hr>
                                                                            <p>File Pending for Translation -  5</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="element">
                                                                        <div class="col-xs-12 col-sm-12 col-md-12 translatename">
                                                                            <p class="wto-label-bold blue-color name">Shally Ahuja</p>
                                                                            <p>Proficient in Language - <b>Chinese, Roman</b></p>
                                                                            <hr>
                                                                            <p>File Pending for Translation -  3</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="element">
                                                                        <div class="col-xs-12 col-sm-12 col-md-12 translatename">
                                                                            <p class="wto-label-bold blue-color name">Goutam Ahuja</p>
                                                                            <p>Proficient in Language - <b>Chinese, Roman</b></p>
                                                                            <hr>
                                                                            <p>File Pending for Translation -  0</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <p class="wto-label-normal top-offset-10"> Sent document for translation on <span class="wto-label-bold">29-December-2017</span></p>
                                                    <a href="#" class="btn sky-blue">Send to Translator</a>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10 selectlangusge">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Remainder for Translation<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        <input type="text" class="form-control date-picker textboxcontrol" id="remainder" />
                                                        <i class="glyphicon glyphicon-calendar form-control-feedback blue-color"></i>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row selectlangusge">
                                                <div class="col-xs-12 col-sm-offset-4 col-sm-8 col-md-offset-3 col-md-9">
                                                    <p class="text-danger hidden" id="RemainderforTranslationError" style="font-size:12px;">Remainder for Translation date is lesser than date of notificaton please check !</p>
                                                </div>
                                            </div>
                                            <div class="row selectlangusge">
                                                <div class="col-xs-12 col-sm-4 col-md-3"></div>
                                                <div class="col-xs-12 col-sm-8 col-md-9">
                                                    <p class="wto-label-normal">
                                                        Translation remainder due in<span class="wto-label-bold">3 Days</span>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row top-offset-10 selectlangusge duedate">
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-bold">Due Date for Translation<sup class="mandetory">*</sup> <span class="pull-right">:</span></p>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-3">
                                                    <p class="wto-label-normal">
                                                        <input type="text" class="form-control date-picker textboxcontrol" id="duedate" />
                                                        <i class="glyphicon glyphicon-calendar form-control-feedback blue-color"></i>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row selectlangusge">
                                                <div class="col-xs-12 col-sm-offset-4 col-sm-8 col-md-offset-3 col-md-9">
                                                    <p class="wto-label-normal text-danger hidden" id="DueDateError" style="font-size:12px;">Due Date for Translation date is lesser than date of notificaton please check !</p>
                                                </div>
                                            </div>
